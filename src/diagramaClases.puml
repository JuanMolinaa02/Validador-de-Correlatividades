@startuml

class Inscripcion{
    - materias: List<Materia>
    - alumno: Alumno

    + aprobada() : boolean
}

class Materia{
    - nombre: String
    - correlativasNecesarias: List<Materia>
}

class Alumno{
    - materiasAprobadas: List<Materia>

    + puedeCursar(Materia materia) : boolean
}

Alumno --> " * " Materia
Inscripcion --> Alumno
Inscripcion --> " * " Materia

@enduml